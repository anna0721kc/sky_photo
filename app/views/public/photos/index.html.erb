<!--photo galleryページ-->

<div class="gallery container my-5 text-center">
    <h1 class="photos my-3">Gallery</h1>
     <div class="photos">
        <% @photos.each do |photo| %>
         <%= link_to photo_path(photo) do %>
         <%= attachment_image_tag photo, :image, :fill, 500, 400, format: 'jpeg' %>
         <% end %>
        <% end %>
     </div>
    <p><%= paginate @photos %></p>
</div>
