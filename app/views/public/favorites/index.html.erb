<!--ユーザーのお気に入り一覧-->
<div class="container my-5 text-center">
  <h3>your favorites</h3>
  <div class="photos">
  <% @favorite_list.each do |photo| %>
  <%= link_to photo_path(photo) do %>
  <%= attachment_image_tag photo, :image, :fill, 500, 400, format: 'jpeg' %>
  <% end %>
  <% end %>
  </div>
</div>